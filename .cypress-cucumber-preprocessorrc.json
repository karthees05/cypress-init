{
  "json": {
    "enabled": true,
    "output": "jsonlogs/log.json",
    "formatter": "cucumber-json-formatter.exe"
  },
  "message": {
    "enabled": true,
    "output": "jsonlogs/message.ndjson"
  },
  "html": {
    "enabled": false
  },
  "cypress-cucumber-preprocessor": {
    "filterSpecs": true,
    "omitFiltered": true
  },
  "stepDefinitions": [
    "cypress/e2e/step_definitions/*.js"
  ]
}
